      .card
        #headingTechnicalInterview.card-header
          h5.mb-0
            button.btn.btn-link.collapsed(data-toggle='collapse', data-target='#collapseTechnicalInterview', aria-expanded='false', aria-controls='collapseTechnicalInterview')
