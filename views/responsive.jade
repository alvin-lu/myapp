extends layout

block content
  h1= 'a/A Jumpstart'
  .container
    #accordion
      .card
        #headingCurriculum.card-header
          h5.mb-0
            button.btn.btn-link(data-toggle='collapse', data-target='#collapseCurriculum', aria-expanded='true', aria-controls='collapseCurriculum')
              | Curriculum
        #collapseCurriculum.collapse.show(aria-labelledby='headingCurriculum', data-parent='#accordion')
          .card-body
            .row
              .col-md-3
                p(class='cardcardLabel') Enumerables 1
                .col-md-12
                  a(href='/ruby/other/curriculum/enumerables1/main.rb')
                    p(class='content') main.rb
                  a(href='/ruby/other/curriculum/enumerables1/test.rb')
                    p(class='content') test.rb
              .col-md-3
                p(class='cardcardLabel') Enumerables 2
                .col-md-12
                  a(href='/ruby/other/curriculum/enumerables2/main.rb')
                    p(class='content') main.rb
                  a(href='/ruby/other/curriculum/enumerables2/test.rb')
                    p(class='content') test.rb
              .col-md-3
                p(class='cardcardLabel') Hashes
                .col-md-12
                  a(href='/ruby/other/curriculum/hashes/main.rb')
                    p(class='content') main.rb
                  a(href='/ruby/other/curriculum/hashes/test.rb')
                    p(class='content') test.rb
      .card
        #headingStepTwo.card-header
          h5.mb-0
            button.btn.btn-link.collapsed(data-toggle='collapse', data-target='#collapseStepTwo', aria-expanded='false', aria-controls='collapseStepTwo')
              | Step 2
        #collapseStepTwo.collapse(aria-labelledby='headingStepTwo', data-parent='#accordion')
          .card-body
            .row
              .col-md-3
                p(class='cardLabel') w1d1
                .col-md-12
                  a(href='/ruby/steptwo/w1d1/homework.rb')
                    p(class='content') homework.rb
              .col-md-3
                p(class='cardLabel') w1d2
                .col-md-12
                  a(href='/ruby/steptwo/w1d2/homework.rb')
                    p(class='content') homework.rb
                .col-md-12
                  a(href='/ruby/steptwo/w1d2/warmup.rb')
                    p(class='content') warmup.rb
              .col-md-3
                p(class='cardLabel') w1d3
                .col-md-12
                  a(href='/ruby/steptwo/w1d3/homework.rb')
                    p(class='content') homework.rb
                .col-md-12
                  a(href='/ruby/steptwo/w1d3/warmup.rb')
                    p(class='content') warmup.rb
              .col-md-3
                p(class='cardLabel') w1d4
                .col-md-12
                  a(href='/ruby/steptwo/w1d4/homework.rb')
                    p(class='content') homework.rb
                .col-md-12
                  a(href='/ruby/steptwo/w1d4/warmup.rb')
                    p(class='content') warmup.rb
            .row
              .col-md-3
                p(class='cardLabel') w2d1
                .col-md-12
                  a(href='/ruby/steptwo/w2d1/homework.rb')
                    p(class='content') homework.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d1/warmup.rb')
                    p(class='content') warmup.rb
              .col-md-3
                p(class='cardLabel') w2d2
                .col-md-12
                  a(href='/ruby/steptwo/w2d2/homework.rb')
                    p(class='content') homework.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d2/question1.rb')
                    p(class='content') question1.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d2/question2.rb')
                    p(class='content') question2.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d2/question3.rb')
                    p(class='content') question3.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d2/question4.rb')
                    p(class='content') question4.rb
              .col-md-3
                p(class='cardLabel') w2d3
                .col-md-12
                  p(class='content') no work was done
              .col-md-3
                p(class='cardLabel') w2d4
                .col-md-12
                  a(href='/ruby/steptwo/w2d4/hashtagify.rb')
                    p(class='content') hashtagify.rb
                .col-md-12
                  a(href='/ruby/steptwo/w2d4/vigenere.rb')
                    p(class='content') vigenere.rb
      .card
        #headingPracticeAssessment.card-header
          h5.mb-0
            button.btn.btn-link.collapsed(data-toggle='collapse', data-target='#collapsePracticeAssessment', aria-expanded='false', aria-controls='collapsePracticeAssessment')
